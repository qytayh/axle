{
  "name": "@varlet/axle",
  "version": "0.2.3",
  "description": "Progressive request tool based on axios",
  "type": "module",
  "main": "es/index.js",
  "module": "es/index.js",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./es/*"
      ]
    }
  },
  "exports": {
    ".": "./es/index.js",
    "./use": "./es/use.js",
    "./*": "./*"
  },
  "files": [
    "es"
  ],
  "keywords": [
    "axle",
    "axios",
    "helpers"
  ],
  "author": "haoziqaq <357229046@qq.com>",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "bootstrap": "pnpm i && pnpm build && pnpm --filter ./playground i",
    "play": " pnpm --filter ./playground dev",
    "dev": "tsc -w",
    "build": "pnpm clear && tsc",
    "release": "pnpm build && vr release",
    "clean": "rimraf ./es"
  },
  "dependencies": {
    "@varlet/shared": "^2.19.2",
    "axios": "^1.4.0",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "@types/qs": "^6.9.7",
    "@varlet/release": "0.0.3",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.5",
    "vue": "3.3.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/varletjs/axle.git"
  },
  "bugs": {
    "url": "https://github.com/varletjs/axle/issues"
  },
  "packageManager": "pnpm@8.7.5",
  "engines": {
    "pnpm": ">=8.0"
  }
}